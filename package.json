{
  "name": "electron-typing-game",
  "version": "1.0.0",
  "main": "dist/main.js",
  "repository": "https://github.com/okayu8/electron-typing-game.git",
  "author": "okayu8 <hitugaya12@gmail.com>",
  "license": "MIT",
  "scripts": {
    "predev": "rimraf dist",
    "?dev": "開発モードでアプリを立ち上げます。",
    "dev": "run-p dev:*",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress",
    "?info": "各スクリプトの説明文を表示します。",
    "info": "npm-scripts-info"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "css-loader": "^6.6.0",
    "electron": "^17.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-search-devtools": "^1.5.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-scripts-info": "^0.3.9",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "wait-on": "^6.0.1"
  }
}
